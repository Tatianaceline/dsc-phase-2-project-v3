    "cursor = conn.cursor()\n",
    "\n",
    "# Create tables\n",
    "bom_selected.to_sql('bom', conn, if_exists='replace', index=False)\n",
    "tn_selected.to_sql('tn', conn, if_exists='replace', index=False)\n",
    "tmdb_df.to_sql('tmdb', conn, if_exists='replace', index=False)"
